{
    "name": "veeqtoh/cashier-paystack",
    "description": "A complete re-write of the abandoned webong-cashier which provides an expressive, fluent interface to Paystack's subscription billing services.",
    "type": "library",
    "homepage": "https://github.com/veeqtoh/cashier-paystack",
    "license": "MIT",
    "authors": [
        {
            "name": "Victor Ukam",
            "email": "victorjohnukam@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "keywords": [
        "veeqtoh",
        "victor-ukam",
        "paystack",
        "cashier",
        "payment",
        "nigeria",
        "africa",
        "naira",
        "laravel",
        "laravel-package"
    ],
    "require": {
        "php": "^8.1",
        "nesbot/carbon": "^2.0|^3.0",
        "illuminate/container": "^10.0|^11.0",
        "illuminate/database": "^10.0|11.0",
        "unicodeveloper/laravel-paystack": "^1.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^8.0|9.0",
        "larastan/larastan": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Veeqtoh\\CashierPaystack\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Veeqtoh\\CashierPaystack\\Providers\\CashierPaystackProvider"
            ],
            "aliases": {
                "CashierPaystack": "Veeqtoh\\CashierPaystack\\Facades\\CashierPaystack"
            }
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test": "vendor/bin/pest"
    }
}
